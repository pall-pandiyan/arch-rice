        fg[PRELIGHT] 	= @text_color
		fg[ACTIVE]      = @text_color
        fg[INSENSITIVE] = "#ADADAD"
}
class "GtkItem"                          style "gimp-button"
widget "*Gimp*Button*"                   style "gimp-button"


style "Gimp-Tool-Palette"
{
    GtkButton::focus-line-width    		= 0
	GtkButtonBox::child-min-height 		= 24
	GtkButton::inner-border	            = { 2,2,2,2 }

	ythickness        = 3
	xthickness        = 3

	bg_pixmap[NORMAL]   = "Assets/bg.svg"

        # buttons:
        bg[NORMAL] 		= "#8C9090"
        bg[PRELIGHT] 	= "#8C9090"
		bg[ACTIVE]      = @selected_bg_color
        bg[INSENSITIVE] = "#606464"
        fg[NORMAL] 		= @text_color
        fg[PRELIGHT] 	= @text_color
		fg[ACTIVE]      = @text_color
        fg[INSENSITIVE] = "#ADADAD"
}
widget "*GimpToolPalette*"                     style "Gimp-Tool-Palette"
