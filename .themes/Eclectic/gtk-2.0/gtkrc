style "checkradio"
{
        fg[NORMAL] 		= @fg_color
        fg[PRELIGHT]    = @text-prelight
        fg[ACTIVE]      = @text_color
        fg[INSENSITIVE] = "#9E9E9E"


        # check/radio
        base[NORMAL] 		= @selected_fg_color
