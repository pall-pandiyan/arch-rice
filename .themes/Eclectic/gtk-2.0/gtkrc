style "entry"
{
    xthickness = 3
	ythickness = 3

			# Normal:
			base[ACTIVE]	    = @selected_bg_color
            text[NORMAL]    	= @selected_fg_color

			# Selected:
            base[SELECTED]	    = @selected_fg_color
            text[SELECTED]	    = "#000000"

			# Active:
			base[ACTIVE]	    = @selected_bg_color
			text[ACTIVE]	    = @selected_fg_color

			# insensitive:
			base[INSENSITIVE]   = "#606464"
            text[INSENSITIVE] 	= "#ADADAD"

    engine "pixmap" 
    {
        image {
        function 	= SHADOW
        detail   	= "entry"
        state    	= NORMAL
        shadow   	= IN
        file     	= "Entry/entry-normal.svg"
        border   	= { 4,4,3,3 }
        stretch  	= TRUE
        }
        image {
        function 	= SHADOW
        detail   	= "entry"
        state    	= INSENSITIVE
        shadow   	= IN
        file     	= "Entry/entry-insensitive.svg"
        border   	= { 4,4,3,3 }
        stretch  	= TRUE
        }
        image {
        function 	= SHADOW
        detail   	= "entry"
        state    	= ACTIVE
        file     	= "Entry/entry-active.svg"
        border   	= { 4,4,3,3 }
        stretch  	= TRUE
        }
        
            image {
            function        = FLAT_BOX
            detail          = "entry_bg"
            state           = NORMAL
            overlay_file    = "Entry/bg_entry.png"
            overlay_border  = { 0,0,0,0 }
            overlay_stretch = TRUE
            }
            image {
            function        = FLAT_BOX
            detail          = "entry_bg"
            state           = INSENSITIVE
            overlay_file    = "Entry/bg_entry_insensitive.png"
            overlay_border  = { 0,0,0,0 }
            overlay_stretch = TRUE
            }
            image {
            function        = FLAT_BOX
            detail          = "entry_bg"
            state           = ACTIVE
            overlay_file    = "Entry/bg_entry_active.png"
            overlay_border  = { 0,0,0,0 }
            overlay_stretch = TRUE
            }
    }
