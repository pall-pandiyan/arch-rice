# How to draw boxes with a gap on one side (ie the page of a notebook)

		image 
		{
		function			= BOX_GAP
		file				= "Notebook/notebook-top.svg" 
		border				= { 1,1,3,1 }
		stretch				= TRUE
		gap_file   			= "Notebook/border-tab-active.svg"
		gap_border 			= { 0,0,2,0 }
		gap_side			= TOP
		}

		image 
		{
		function			= BOX_GAP
		file				= "Notebook/notebook-bottom.svg"
		border				= { 1,1,1,3 }
		stretch				= TRUE
		gap_file   			= "Notebook/border-tab-active.svg"
		gap_border 			= { 0,0,0,2 }
		gap_side			= BOTTOM
		}

		image 
		{
		function			= BOX_GAP
		file				= "Notebook/notebook-left.svg"
		border				= { 3,1,1,1 }
		stretch				= TRUE
		gap_file   			= "Notebook/border-tab-active.svg"
		gap_border 			= { 2,0,0,0 }
		gap_side			= LEFT
		}

		image 
		{
		function			= BOX_GAP
		file				= "Notebook/notebook-right.svg" 
		border				= { 1,3,1,1 }
		stretch				= TRUE
		gap_file   			= "Notebook/border-tab-active.svg"
		gap_border 			= { 0,2,0,0 }
		gap_side			= RIGHT
		}

