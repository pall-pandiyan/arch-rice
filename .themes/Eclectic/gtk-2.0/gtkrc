style "spinbutton"
{
	xthickness	= 4
	ythickness 	= 3

	bg[NORMAL] = @bg_color

			# Normal:
            base[NORMAL] 		= @selected_bg_color
            text[NORMAL]    	= @selected_fg_color

			# Selected:
            base[SELECTED]	    = @selected_fg_color
            text[SELECTED]	    = "#000000"

			# Active:
			base[ACTIVE]	    = @selected_bg_color
			text[ACTIVE]	    = @selected_fg_color

			# insensitive:
			base[INSENSITIVE]   = "#606464"
            text[INSENSITIVE] 	= "#ADADAD"


			# Progressbar:
			bg[ACTIVE]	    = @selected_bg_color 	# Trough
			fg[NORMAL]      = @text_color
			bg[SELECTED]	= "#3C4A58" 			# Progress-Bar
			fg[PRELIGHT]    = @text_color
			fg[SELECTED] 	= @text_color

			# spinbuttons:
			bg[NORMAL] 		= @selected_bg_color
			bg[PRELIGHT] 	= @selected_bg_color

	engine "pixmap"
	{
		image { function = ARROW }

		image
		{
		function			= BOX
        state               = NORMAL
		detail				= "spinbutton_up"
		file				= "SpinButton/spin_button_up.svg"
		border				= { 1,3,3,1 }
		stretch				= TRUE
        overlay_file		= "SpinButton/arrow-up.svg"
        overlay_border		= { 0,0,0,0 }
        overlay_stretch		= FALSE
		}

		image
		{
		function			= BOX
        state               = NORMAL
		detail				= "spinbutton_down"
		file				= "SpinButton/spin_button_down.svg"
		border				= { 1,3,1,3 }
		stretch				= TRUE
        overlay_file		= "SpinButton/arrow-down.svg"
        overlay_border		= { 0,0,0,0 }
        overlay_stretch		= FALSE
		}

		image
		{
		function			= BOX
        state               = PRELIGHT
		detail				= "spinbutton_up"
		file				= "SpinButton/spin_button_up.svg"
		border				= { 1,3,3,1 }
		stretch				= TRUE
        overlay_file		= "SpinButton/arrow-up-prelight.svg"
        overlay_border		= { 0,0,0,0 }
        overlay_stretch		= FALSE
		}

		image
		{
		function			= BOX
        state               = PRELIGHT
		detail				= "spinbutton_down"
		file				= "SpinButton/spin_button_down.svg"
		border				= { 1,3,1,3 }
		stretch				= TRUE
        overlay_file		= "SpinButton/arrow-down-prelight.svg"
        overlay_border		= { 0,0,0,0 }
        overlay_stretch		= FALSE
		}

		image
		{
		function			= BOX
        state               = ACTIVE
		detail				= "spinbutton_up"
		file				= "SpinButton/spin_button_up.svg"
		border				= { 1,3,3,1 }
		stretch				= TRUE
        overlay_file		= "SpinButton/arrow-up-active.svg"
        overlay_border		= { 0,0,0,0 }
        overlay_stretch		= FALSE
		}

		image
		{
		function			= BOX
        state               = ACTIVE
		detail				= "spinbutton_down"
		file				= "SpinButton/spin_button_down.svg"
		border				= { 1,3,1,3 }
		stretch				= TRUE
        overlay_file		= "SpinButton/arrow-down-active.svg"
        overlay_border		= { 0,0,0,0 }
        overlay_stretch		= FALSE
		}

		image
		{
		function			= BOX
        state               = INSENSITIVE
		detail				= "spinbutton_up"
		file				= "SpinButton/spin_button_up.svg"
		border				= { 1,3,3,1 }
		stretch				= TRUE
        overlay_file		= "SpinButton/arrow-up-insensitive.svg"
        overlay_border		= { 0,0,0,0 }
        overlay_stretch		= FALSE
		}

		image
		{
		function			= BOX
        state               = INSENSITIVE
		detail				= "spinbutton_down"
		file				= "SpinButton/spin_button_down.svg"
		border				= { 1,3,1,3 }
		stretch				= TRUE
        overlay_file		= "SpinButton/arrow-down-insensitive.svg"
        overlay_border		= { 0,0,0,0 }
        overlay_stretch		= FALSE
		}


	# SPIN ENTRY ##############
        image {
        function 	= SHADOW
        detail   	= "entry"
        state    	= NORMAL
        shadow   	= IN
        file     	= "SpinButton/spin-entry.svg"
        border   	= { 4,0,3,3 }
        stretch  	= TRUE
        }
        image {
        function 	= SHADOW
        detail   	= "entry"
        state    	= INSENSITIVE
        shadow   	= IN
        file     	= "SpinButton/spin-entry-insensitive.svg"
        border   	= { 4,0,3,3 }
        stretch  	= TRUE
        }
        image {
        function 	= SHADOW
        detail   	= "entry"
        state    	= ACTIVE
        file     	= "SpinButton/spin-entry-active.svg"
        border   	= { 4,0,3,3 }
        stretch  	= TRUE
        }

            image {
            function        = FLAT_BOX
            detail          = "entry_bg"
            state           = NORMAL
            overlay_file    = "SpinButton/bg_entry_active.png"
            overlay_border  = { 0,0,0,0 }
            overlay_stretch = TRUE
            }
            image {
            function        = FLAT_BOX
            detail          = "entry_bg"
            state           = INSENSITIVE
            overlay_file    = "SpinButton/bg_entry_insensitive.png"
            overlay_border  = { 0,0,0,0 }
            overlay_stretch = TRUE
            }
            image {
